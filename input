1 2 3 4 5 6 7; // Estados
0 1; // Alfabeto
//	Transições
2 3, //	do estado 1 sob 0 leva o ao estado 2, e sob 1 leva ao estado 3
1 3, // do estado 2 sob 0 leva ao estado 1, e sob 1 leva ao estado 3
4 3,
5 6,
5 7,
7 6,
5 7;

1; // Estado inicial	
4 6; // Estados finais

